                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    header="FS to ABAP Chart of Effort/Day"
                                >
                                    <TileContent
                                    footer="47 Days Effort Savings"
                                    >
                                        <mc:LineMicroChart
                                            threshold="12"
                                            size="Responsive"
                                            showPoints="true"
                                            color="{above: 'Good', below: 'Error'}">

                                            <mc:lines>
                                                <mc:LineMicroChartLine
                                                >
                                                    <mc:LineMicroChartPoint
                                                        x="0"
                                                        y="35"
                                                    />
                                                      <mc:LineMicroChartPoint
                                                        x="20"
                                                        y="53"
                                                    />
                                                      <mc:LineMicroChartPoint
                                                        x="40"
                                                        y="10"
                                                    />
                                                      <mc:LineMicroChartPoint
                                                        x="60"
                                                        y="30"
                                                    />
                                                      <mc:LineMicroChartPoint
                                                        x="80"
                                                        y="52"
                                                    />
                                                </mc:LineMicroChartLine>
                                            </mc:lines>
                                        </mc:LineMicroChart>
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="Effort Savings for ABAP to FS"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent
                                    footer="27 Days Effort Savings"
                                    >
                                        <NumericContent
                                            scale="D"
                                            value="27"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>

                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="Daily Application Usage"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent
                                        footer="Current Quarter"
                                    >
                                        <NumericContent
                                            scale="Employees"
                                            value="0"
                                            valueColor="Critical"
                                            indicator="None"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    header="Employees/Number of Employees Using the Application"
                                    subheader="Monthly"
                                    frameType="TwoByOne"
                                >
                                    <TileContent
                                      
                                        footer="ABAP to TS"
                                    >
                                        <content>
                                            <mc:HarveyBallMicroChart
                                                total="180"
                                                size="Responsive"
                                                alignContent="Right"
                                                totalScale="P"
                                            >
                                                <mc:items>
                                                    <mc:HarveyBallMicroChartItem
                                                        fraction="143"
                                                        color="Critical"
                                                        fractionScale="P"
                                                    />
                                                </mc:items>
                                            </mc:HarveyBallMicroChart>
                                        </content>
                                    </TileContent>
                                    <TileContent
                   
                                        footer="FS to ABAP"
                                    >
                                        <content>
                                            <mc:HarveyBallMicroChart
                                                total="127"
                                                size="Responsive"
                                                alignContent="Right"
                                                totalScale="P"
                                            >
                                                <mc:items>
                                                    <mc:HarveyBallMicroChartItem
                                                        fraction="118"
                                                        color="Good"
                                                        fractionScale="P"
                                                    />
                                                </mc:items>
                                            </mc:HarveyBallMicroChart>
                                        </content>
                                    </TileContent>
                                </GenericTile>
                            </HBox>
                        </VBox>
                    </l:BlockLayoutCell>
