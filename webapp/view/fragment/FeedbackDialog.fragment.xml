<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        title="{i18n>Feedback}"
        icon="sap-icon://feedback"
    >
        <content>
            <VBox
                fitContainer="true"
                class="sapUiMediumMargin"
            >
                <!-- <Label text="{i18n>Name}" />
                <Input value="{/Name}" />
                <Label text="{i18n>Surname}" />
                <Input value="{/Surname}" /> -->
                <Label text="{i18n>Feedback}" />
                <TextArea
                    id="content"
                    width="100%"
                    growing="true"
                />
            </VBox>
        </content>
        <beginButton>
            <Button
                text="{i18n>Save}"
                press="onSaveFB"
            />
        </beginButton>
        <endButton>
            <Button
                text="{i18n>Close}"
                press="onPressCloseFB"
            />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
