        <f:content>
            <l:BlockLayout id="blockLayout">
                <l:BlockLayoutRow>
                    <l:BlockLayoutCell title="Overview">
                        <VBox>
                            <Title text="Insights and Monitoring" />
                            <HBox>
                                <GenericTile
                                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                                    header="Generate TS/ABAP from Functional Spec"
                                    press="onPressFsToAbap"
                                >
                                    <TileContent footer="Current Quarter">
                                        <NumericContent
                                            scale="FS"
                                            value="125"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    frameType="OneByOne"
                                    header="Generate FS/TS from Programs"
                                    press="onPressAbapToFs"
                                >
                                    <TileContent footer="Current Quarter">
                                        <NumericContent
                                            scale="P"
                                            value="95"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <!-- </l:BlockLayoutCell> -->
                                <!-- <l:BlockLayoutCell class="customCellImageBackground" /> -->
                                <!-- <l:BlockLayoutCell> -->
                                <SlideTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                >
                                    <tiles>
                                        <GenericTile
                                            backgroundImage="https://nttdata-solutions.com/wp-content/usermedia/serdalbey.png"
                                            frameType="TwoByOne"
                                            press="window.open('https://nttdata-solutions.com/tr/press-centre/local-press-releases/podcast-pods-of-business/')"
                                        >
                                            <TileContent footer="11/01/2024">
                                                <NewsContent
                                                    contentText="🎙️Pods of Business Podcast Series is Live!"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                        <GenericTile
                                            backgroundImage="https://nttdata-solutions.com/wp-content/usermedia/Bahri-Danis-Altin-Lider-Odulu.jpg"
                                            frameType="TwoByOne"
                                            press="window.open('https://nttdata-solutions.com/tr/press-centre/local-press-releases/ceomuz-dr-bahri-danis-bu-sene-de-turkiyenin-en-begenilen-50-ceosundan-biri-oldu/')"
                                        >
                                            <TileContent footer="27/12/2023">
                                                <NewsContent
                                                    contentText="Our CEO Dr. Bahri Danış is one of Turkey's 50 Most Admired CEOs this year!"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                    </tiles>
                                </SlideTile>
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="FS to ABAP"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent
                                        footer="Code Created"
                                    >
                                        <NumericContent
                                            scale="ABAP"
                                            value="120"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="FS to TS"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent
                                    footer="Documents Created"
                                    >
                                        <NumericContent
                                            scale="TS"
                                            value="95"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                            </HBox>
                            <HBox>
                                <GenericTile
                                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="Effort for FS to TS"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent unit="Effort savings achieved">
                                        <mc:BulletMicroChart
                                            size="S"
                                            scale="%"
                                            forecastValue="100"
                                            class="sapUiSmallMargin"
                                            press="press"
                                        >
                                            <mc:actual>
                                                <mc:BulletMicroChartData
                                                    value="83"
                                                    color="Good"
                                                />
                                            </mc:actual>
                                            <mc:thresholds>
                                                <mc:BulletMicroChartData
                                                    value="0"
                                                    color="Error"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="25"
                                                    color="Critical"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="50"
                                                    color="Critical"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="75"
                                                    color="Error"
                                                />
                                            </mc:thresholds>
                                        </mc:BulletMicroChart>
                                    </TileContent>
                                </GenericTile>

                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    header="ABAP to FS/TS"
                                    frameType="TwoByOne"
                                >
                                    <TileContent>
                                        <mc:ComparisonMicroChart
                                            scale="P"
                                            view="Wide"
                                            size="Responsive"
                                        >
                                            <mc:data>
                                                <mc:ComparisonMicroChartData
                                                    title="FS"
                                                    value="125"
                                                    color="Good"
                                                />
                                                <mc:ComparisonMicroChartData
                                                    title="TS"
                                                    value="95"
                                                    color="Critical"
                                                />
                                                <!-- <mc:ComparisonMicroChartData
                                                    title="APAC"
                                                    value="-20"
                                                    color="Error"
                                                /> -->
                                            </mc:data>
                                        </mc:ComparisonMicroChart>
                                    </TileContent>
                                </GenericTile>

                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="Effort for FS to ABAP"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent unit="ABAP">
                                        <mc:BulletMicroChart
                                            size="S"
                                            scale="%"
                                            forecastValue="100"
                                            class="sapUiSmallMargin"
                                            press="press"
                                        >
                                            <mc:actual>
                                                <mc:BulletMicroChartData
                                                    value="76"
                                                    color="Critical"
                                                />
                                            </mc:actual>
                                            <mc:thresholds>
                                                <mc:BulletMicroChartData
                                                    value="0"
                                                    color="Error"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="25"
                                                    color="Critical"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="50"
                                                    color="Critical"
                                                />
                                                <mc:BulletMicroChartData
                                                    value="75"
                                                    color="Error"
                                                />
                                            </mc:thresholds>
                                        </mc:BulletMicroChart>
                                    </TileContent>
                                </GenericTile>
                                
                                <GenericTile
                                    class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
                                    size="L"
                                    header="ABAP to FS"
                                    press="press"
                                    mode="ArticleMode"
                                >
                                    <TileContent
                                        footer="Documents Created"
                                    >
                                        <NumericContent
                                            scale="FS"
                                            value="90"
                                            valueColor="Good"
                                            indicator="Up"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                            </HBox>
                        </VBox>
                    </l:BlockLayoutCell>
                </l:BlockLayoutRow>
                <l:BlockLayoutRow>
                    <l:BlockLayoutCell>
                        <VBox>
                            <Title text="Working Space" />
