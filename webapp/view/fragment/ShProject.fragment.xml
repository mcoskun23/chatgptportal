<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        contentWidth="auto"
        contentHeight="auto"
        title="{i18n>ProjectName}"
        search=".onSearchProject"
        confirm=".onProjectConfirm"
        cancel=".onProjectClose"
        items="{
			path: '/ShProjectSet'
		}"
    >
        <StandardListItem
            title="{Projecttxt}"
            description="{Projectid}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
